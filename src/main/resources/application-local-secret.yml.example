# 🚨 이것은 예시 파일입니다
# 실제 사용을 위해서는:
# 1. 이 파일을 application-local-secret.yml로 복사
# 2. 아래 값들을 실제 값으로 변경
# 3. application-local-secret.yml은 절대 GitHub에 커밋하지 마세요!

spring:
  datasource:
    password: "your-real-database-password"

jwt:
  secret: "your-real-jwt-secret-key-that-must-be-very-long-and-secure"

openai:
  api:
    key: "your-real-openai-api-key-starting-with-sk-"

kakao:
  api:
    rest-key: "your-real-kakao-rest-api-key"

cloud:
  aws:
    credentials:
      access-key: "your-real-aws-access-key"
      secret-key: "your-real-aws-secret-key"
