# 데이터베이스 설정
DB_URL=jdbc:mysql://localhost:3306/mapoop?useSSL=false&serverTimezone=Asia/Seoul&allowPublicKeyRetrieval=true&useUnicode=true&characterEncoding=utf8
DB_USERNAME=your_db_username
DB_PASSWORD=your_db_password

# Redis 설정
REDIS_HOST=localhost
REDIS_PORT=6379

# JWT 설정
JWT_SECRET=your_jwt_secret_key_here_minimum_32_characters_long

# AWS S3 설정 (선택사항)
AWS_ACCESS_KEY=your_aws_access_key
AWS_SECRET_KEY=your_aws_secret_key
AWS_REGION=ap-northeast-2
AWS_S3_BUCKET=mapoop-bucket

# OpenAI API 설정
OPENAI_API_KEY=your_openai_api_key_here
